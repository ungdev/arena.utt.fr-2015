            <div class="panel container" id="register">
                <h1>Inscription</h1>
                <p>Le nombre de place est limité n'hésitez pas à vous inscrire.</p>
                {{ form_start(registration) }}
                {{ form_errors(registration) }}
                {{ form_row(registration.player) }}
                <div>
                    {{ form_errors(registration.rules) }}
                    <label>
                        {{ form_widget(registration.rules) }}
                        &nbsp;J'accepte sans conditions <a href="#" data-modal="rules-modal">le règlement du tournoi</a>.
                    </label>
                </div>
                <div>
                    {{ form_errors(registration.cgu) }}
                    <label>
                        {{ form_widget(registration.cgu) }}
                        &nbsp;J'ai lu et j'accepte les <a href="{{ path('terms') }}">conditions générales d'utilisation</a>.
                    </label>
                </div>
                <button type="submit" class="button">S'inscrire</button>
                {{ form_end(registration) }}
            </div>
        </section>
        {% endif %}

        {% if not registrable %}
            <section class="module parallax parallax-6"></section>
            <section class="module content">
                <div class="panel container" id="register">
                    <h1>Inscription</h1>
                    <p>Nous sommes désolés mais les inscription sont terminées. Merci de votre interêt pour l'évenement.</p>
                </div>
            </section>
        {% endif %}
    </main>
{% endblock %}

{% block javascripts %}
<script>
    if (window.location.pathname == '{{ path('register') }}') {
        window.location.hash = 'register';
    }
</script>
{% endblock %}
